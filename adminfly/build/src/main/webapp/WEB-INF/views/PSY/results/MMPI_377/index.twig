{% extends '../../layout.twig' %}
{% block content %}
    <div class="page-header mb-4">
        <h1>{{ result.patient.name }}</h1>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <h3>{{ result.test }}</h3>

            <div class="card bg-light mb-3">
                <div class="card-header">{{ 'label.results' | translate }}</div>
                <div class="card-body">
                    {{ result.patient.name }}
                </div>
            </div>

            <img src="{{ values.get().getBase64PNGChart() }}" alt="">

        </div>
    </div>
{% endblock %}